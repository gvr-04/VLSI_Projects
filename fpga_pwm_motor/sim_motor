#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x570e4ad200e0 .scope module, "motor_tb" "motor_tb" 2 4;
 .timescale -9 -12;
v0x570e4ad341b0_0 .var "clk", 0 0;
v0x570e4ad34280_0 .net "motor", 2 0, v0x570e4ad33f60_0;  1 drivers
v0x570e4ad34350_0 .var "switch", 2 0;
S_0x570e4ad20270 .scope module, "uut" "motor" 2 11, 3 1 0, S_0x570e4ad200e0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 3 "switch";
    .port_info 2 /OUTPUT 3 "motor";
P_0x570e4ace4490 .param/l "cycle_len" 0 3 7, +C4<00000000000000000000000000001010>;
P_0x570e4ace44d0 .param/l "duty0" 0 3 8, +C4<00000000000000000000000000000100>;
P_0x570e4ace4510 .param/l "duty1" 0 3 9, +C4<00000000000000000000000000001000>;
v0x570e4ad20c90_0 .net "clk", 0 0, v0x570e4ad341b0_0;  1 drivers
v0x570e4ad1d5f0_0 .var/i "counter", 31 0;
v0x570e4ad33f60_0 .var "motor", 2 0;
v0x570e4ad34050_0 .net "switch", 2 0, v0x570e4ad34350_0;  1 drivers
E_0x570e4ace4910 .event anyedge, v0x570e4ad34050_0, v0x570e4ad1d5f0_0;
E_0x570e4ad1da30 .event posedge, v0x570e4ad20c90_0;
    .scope S_0x570e4ad20270;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x570e4ad1d5f0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x570e4ad20270;
T_1 ;
    %wait E_0x570e4ad1da30;
    %load/vec4 v0x570e4ad1d5f0_0;
    %cmpi/s 9, 0, 32;
    %jmp/0xz  T_1.0, 5;
    %load/vec4 v0x570e4ad1d5f0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x570e4ad1d5f0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x570e4ad1d5f0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x570e4ad20270;
T_2 ;
    %wait E_0x570e4ace4910;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x570e4ad33f60_0, 0, 3;
    %load/vec4 v0x570e4ad34050_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x570e4ad34050_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0 T_2.2, 8;
    %pushi/vec4 0, 0, 2;
    %jmp/1 T_2.3, 8;
T_2.2 ; End of true expr.
    %pushi/vec4 1, 0, 2;
    %jmp/0 T_2.3, 8;
 ; End of false expr.
    %blend;
T_2.3;
    %pad/s 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x570e4ad33f60_0, 4, 1;
    %load/vec4 v0x570e4ad34050_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0 T_2.4, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_2.5, 8;
T_2.4 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_2.5, 8;
 ; End of false expr.
    %blend;
T_2.5;
    %pad/s 1;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x570e4ad33f60_0, 4, 1;
    %load/vec4 v0x570e4ad1d5f0_0;
    %load/vec4 v0x570e4ad34050_0;
    %parti/s 1, 2, 3;
    %flag_set/vec4 8;
    %jmp/0 T_2.8, 8;
    %pushi/vec4 8, 0, 32;
    %jmp/1 T_2.9, 8;
T_2.8 ; End of true expr.
    %pushi/vec4 4, 0, 32;
    %jmp/0 T_2.9, 8;
 ; End of false expr.
    %blend;
T_2.9;
    %cmp/s;
    %flag_mov 8, 5;
    %jmp/0 T_2.6, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_2.7, 8;
T_2.6 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_2.7, 8;
 ; End of false expr.
    %blend;
T_2.7;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x570e4ad33f60_0, 4, 1;
T_2.0 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x570e4ad200e0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x570e4ad341b0_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x570e4ad200e0;
T_4 ;
    %delay 10000, 0;
    %load/vec4 v0x570e4ad341b0_0;
    %inv;
    %store/vec4 v0x570e4ad341b0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x570e4ad200e0;
T_5 ;
    %vpi_call 2 21 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x570e4ad200e0 {0 0 0};
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x570e4ad34350_0, 0, 3;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x570e4ad34350_0, 0, 3;
    %delay 400000, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x570e4ad34350_0, 0, 3;
    %delay 400000, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x570e4ad34350_0, 0, 3;
    %delay 400000, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x570e4ad34350_0, 0, 3;
    %delay 400000, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_motor.v";
    "./motor.v";
